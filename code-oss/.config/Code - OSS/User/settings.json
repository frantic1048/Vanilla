{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "[ruby]": {},
  "[shellscript]": {},
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "atomKeymap.promptV3Features": true,
  "breadcrumbs.enabled": true,
  "editor.columnSelection": false,
  "editor.cursorBlinking": "phase",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Victor Mono Medium",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.formatOnSave": false,
  "editor.formatOnType": false,
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderIndentGuides": false,
  "editor.renderLineHighlight": "gutter",
  "editor.renderWhitespace": "none",
  "editor.tabSize": 2,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "files.associations": {
    "Dangerfile": "ruby"
  },
  "go.formatTool": "goimports",
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "javascript.referencesCodeLens.enabled": false,
  "javascript.referencesCodeLens.showOnAllFunctions": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.validate.enable": false,
  "python.formatting.provider": "yapf",
  "python.formatting.yapfPath": "/usr/bin/yapf",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.tabs.enabled": true,
  "todomd.defaultFile": "/home/chino/Documents/todo.txt",
  "todomd.sortProjectsView": "alphabetic",
  "todomd.webview.completedStrikeThrough": true,
  "typescript.implementationsCodeLens.enabled": false,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.referencesCodeLens.enabled": false,
  "typescript.referencesCodeLens.showOnAllFunctions": false,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "typescript.validate.enable": true,
  "update.mode": "none",
  "window.menuBarVisibility": "hidden",
  "window.titleBarStyle": "native",
  "workbench.activityBar.visible": false,
  "workbench.colorCustomizations": {
    "activityBar.background": "#0000006a",
    "breadcrumb.background": "#00000000",
    "breadcrumbPicker.background": "#000000a0",
    "button.background": "#00000040",
    "button.hoverBackground": "#00000080",
    "editor.background": "#00000022",
    "editor.lineHighlightBackground": "#70E5FF70",
    "editorCursor.foreground": "#6fb9ff",
    "editorGroup.border": "#51abff1a",
    "editorGroupHeader.tabsBackground": "#00000050",
    "editorGutter.background": "#00000030",
    "editorLineNumber.activeForeground": "#FFF",
    "editorLineNumber.foreground": "#aaa",
    "input.background": "#000000a0",
    "input.border": "#51abff",
    "list.activeSelectionBackground": "#00000040",
    "list.hoverBackground": "#00000040",
    "list.inactiveSelectionBackground": "#00000040",
    "panel.background": "#00000080",
    "panel.border": "#00000000",
    "sideBar.background": "#0000006a",
    "sideBarSectionHeader.background": "#00000000",
    "statusBar.background": "#00000080",
    "statusBar.noFolderBackground": "#00000080",
    "tab.activeBackground": "#00000050",
    "tab.border": "#00000000",
    "tab.inactiveBackground": "#00000010",
    "terminal.background": "#0000",
    "titleBar.activeBackground": "#00000030",
    "workbench.background": "#00000080" // only in code-transparent
  },
  "workbench.colorTheme": "Solarized Dark",
  "workbench.editor.showTabs": true,
  "workbench.sideBar.location": "right",
  "workbench.statusBar.visible": true
}
