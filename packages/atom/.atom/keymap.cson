'atom-workspace':
#  'ctrl-alt-o': 'unset!'
  'ctrl-alt-o': 'application:add-project-folder'
  'ctrl-|': 'symbols-tree-view:toggle'
  'alt-s': 'window:save-all'

'.platform-linux':
  # disable default keybinding resolver
  'ctrl-.': 'unset!'

'atom-text-editor:not([mini])':
#  'ctrl-e': 'unset!'
  'ctrl-e' : 'emmet:expand-abbreviation'
  'f10' : 'editor:toggle-soft-wrap' # soft wrap trigger like Kate
  'alt-[': 'tern:definition'
  'alt-]': 'tern:markerCheckpointBack'
  'alt-l': 'tern:references'
  'alt-r': 'tern:rename'
  'alt-p': 'tern:documentation'
  'alt-;': 'tern:startCompletion'
  'ctrl-.': 'linter-tslint:fix-file'
  'ctrl-,': 'prettier:format'
